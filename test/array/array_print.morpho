/*
 * Printing arrays
 */

// Uninitialized array
var b[5]
print b
// expect: [ 0, 0, 0, 0, 0 ]

// Two dimensional array
var c[2,2]
c[0,0]=1
c[1,0]=2
c[0,1]=3
c[1,1]=4
print c
// expect: [ [ 1, 3 ], [ 2, 4 ] ]


c[0,0]=true
c[1,0]=Object()
c[0,1]=false
c[1,1]=nil
print c
// expect: [ [ true, false ], [ <Object>, nil ] ]
