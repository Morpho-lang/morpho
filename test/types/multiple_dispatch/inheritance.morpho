// Multiple dispatch with inheritance

class A {}

class B is A {} 

class C is B {} 

class D { }

class E is D with A { }

fn f(A x) { return "A" }

fn b(B x) { return "B" }

print f(A()) // expect: A 

print f(B()) // expect: B

print f(C()) // expect: B

print f(E()) // expect: A

print f(D()) // expect error 'MltplDsptchFld'
