// CG2 FunctionSpace in one dimension

import meshtools 

var m = LineMesh(fn (t) [t,t], 0..1:0.5) 

var l = FunctionSpace("CG2", grade=1)

var f = Field(m, fn (x,y) x^2+y^2, functionspace=l)

fn integrand(x, u) {
    var g = grad(u)
    print x
    print "-"
    print g 
    print "---"
    return g.norm()^2
}

print LineIntegral(integrand, f, method={ }).total(m) 
// expect: true
