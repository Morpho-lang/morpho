// Initialize different matrices

var a = GPUMatrix([[1]])
print a
// expect: [ 1 ]

var b = GPUMatrix([ [1, 2], [3, 4]])
print b
// expect: [ 1 2 ]
// expect: [ 3 4 ]

var c = GPUMatrix([ [1, 2, 3], [4, 5, 6], [7, 8, 9]])
print c
// expect: [ 1 2 3 ]
// expect: [ 4 5 6 ]
// expect: [ 7 8 9 ]

a = GPUMatrix(2,2)
a[0,0] = 1.0
a[1,1] = 1.0
print a
// expect: [ 1 0 ]
// expect: [ 0 1 ]

var v = GPUMatrix([0.2, 0.3])
print v
// expect: [ 0.2 ]
// expect: [ 0.3 ]

var w = GPUMatrix(b)
print w
// expect: [ 1 2 ]
// expect: [ 3 4 ]

var w = GPUMatrix([[1,[2,3]]])
// expect Error 'MtrxInvldInit'
print w
