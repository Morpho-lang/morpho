class Foo {
  init(arg) {
    print "Foo.init(" + arg + ")"
    self.field = "init"
  }
}

var foo = Foo("one") // expect: Foo.init(one)
foo.field = "field"

var foo2 = foo.init("two") // expect: Foo.init(two)
print foo2 // expect: <Foo>

// Make sure init() doesn't create a fresh instance.
print foo.field // expect: init
