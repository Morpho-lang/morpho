// Reshape matrices 

var a = Matrix([[1,4], [2,5], [3,6]])
print a 
// expect: [ 1 4 ]
// expect: [ 2 5 ]
// expect: [ 3 6 ]

a.reshape(1,6)
print a 
// expect: [ 1 2 3 4 5 6 ]

a.reshape(2,3)
print a 
// expect: [ 1 3 5 ]
// expect: [ 2 4 6 ]

a.reshape(6,1)
print a 
// expect: [ 1 ]
// expect: [ 2 ]
// expect: [ 3 ]
// expect: [ 4 ]
// expect: [ 5 ]
// expect: [ 6 ]

a.reshape(3,2)
print a 
// expect: [ 1 4 ]
// expect: [ 2 5 ]
// expect: [ 3 6 ]

a.reshape(3,3)
// expect error 'MtrxIncmptbl'
